<!-- Show All Poems in one category -->
<header>
  <div class="container title-bar">
    <h2>
      <a href="/categories/">Categories ></a>
      <span class="type-font"> <%= category.name %></span>
    </h2>
  </div>
</header>

<main class="container main">
  <% poems.forEach(poem => { %>

  <h3>
    <a class="type" href="/poems/<%= poem.id %>"><%= poem.title %></a> by
    <a class="highlight" href="/users/<%=poem.user.id%>"><%=poem.user.name%></a>
  </h3>

  <% }); %>
</main>